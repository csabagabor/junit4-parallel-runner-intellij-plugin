<idea-plugin>
    <id>com.gabor.parallel.tester.0.0</id>
    <name>JUnit4 Parallel Tester</name>
    <version>1.0</version>
    <vendor email="csabagabor97@gmail.com" >Csaba Gabor</vendor>

    <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

    <change-notes><![CDATA[
      First version.<br>
      <em>most HTML tags may be used</em>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="93"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->

    <depends>JUnit</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <executor implementation="gabor.paralleltester.executor.CustomRunnerExecutor"/>
        <programRunner implementation="gabor.paralleltester.runner.CustomRunner" order="first"/>
        <executor implementation="gabor.paralleltester.executor.CustomDebuggerExecutor"/>
        <programRunner implementation="gabor.paralleltester.runner.CustomDebuggerRunner" order="last"/>
    </extensions>

</idea-plugin>